<script type="text/javascript">
    $(document).ready(function () {
        $(".visiblePassword").click(function () {
            if ($(this).hasClass("fa-eye-slash")) {
                $(this).parent().find("input[type='password']").attr("type", "text");
                $(this).removeClass("fa-eye-slash");
                $(this).addClass("fa-eye");
            } else {
                $(this).parent().find("input[type='text']").attr("type", "password");
                $(this).removeClass("fa-eye");
                $(this).addClass("fa-eye-slash");
            }
        });

        $(".otpSendButton button").click(function () {
            var mobile = $(this).parent().parent().find("input[name='Student_Mobile']").val();
            $("#sendForgotOTP").find("input[name='Student_Mobile']").val(mobile);
            $("form#sendForgotOTP").submit();
        });
    });

    function loginProcess() {
        $(".errormsg").html("");
    }

    function loginResp(data) {
        console.log(data);
        if (data.errorText == null) {
            //alert(data);
            var url = $("input[name='url']").val();
            if (url != "") {
                window.location.href = url;
            } else {
                window.location.href = "/post";
            }
        }
        else {
            $(".StudentResponse").html("<span class='errorMsg'>" + data.errorText + "</span>");
        }
    }

    function sendForgotOTPResp(resp) {
        if (resp.message != null) {
            $(".otpSendButton button").text("Resend");
            $(".EnterOTP,.forgotPassBtn").removeClass("hide");
        } else {

        }
    }

    //==========Student Forgot Password=========
    function ForgotPassResp(data) {
        if (data.message != null) {
            window.location.href = "/account/change_password/" + data.Student_Id;
        } else {
            $("#notification .modal-body").append("<p><strong>" + data + "</strong></p>");
        }
        $(".modal").modal('hide');
        $("#notification").modal("show");
    }

        
    function ParentLoginResp(data) {
        console.log(data);
        if (data.message != null) {
            //alert(data);
            window.location.href = "/parent";
        }
        else {
            $(".ParentsResponse").html("<span class='errorMsg'>" + data.errorText + "</span>");
        }
    }


    </script>

